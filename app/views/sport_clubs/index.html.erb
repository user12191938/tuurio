<h1>Listing sport_clubs</h1>

<p class="test">
	Hallo Test
</p>
<table>
	<tr>
		<th>Imagepath</th>
		<th>Name</th>
		<th>Description</th>
		<th>Address</th>
		<th>Plz</th>
		<th>City</th>
		<th>Price</th>
		<th>Sporttypes</th>
		<th>Sportfacility</th>
		<th>Traininghours</th>
		<th>News</th>
		<th>Table</th>
		<th>Matchschedule</th>
		<th>Website</th>
		<th>Squad</th>
		<th>Youthsquads</th>
		<th></th>
		<th></th>
		<th></th>
	</tr>

	<% @sport_clubs.each do |sport_club| %>
	<tr>
		<td><%= sport_club.imagePath %></td>
		<td><%= sport_club.name %></td>
		<td><%= sport_club.description %></td>
		<td><%= sport_club.address %></td>
		<td><%= sport_club.plz %></td>
		<td><%= sport_club.city %></td>
		<td><%= sport_club.price %></td>
		<td><%= sport_club.sportTypes %></td>
		<td><%= sport_club.sportFacility %></td>
		<td><%= sport_club.trainingHours %></td>
		<td><%= sport_club.news %></td>
		<td><%= sport_club.table %></td>
		<td><%= sport_club.matchSchedule %></td>
		<td><%= sport_club.website %></td>
		<td><%= sport_club.squad %></td>
		<td><%= sport_club.youthSquads %></td>
		<td><%= link_to 'Show', sport_club %></td>
		<td><%= link_to 'Edit', edit_sport_club_path(sport_club) %></td>
		<td><%= link_to 'Destroy', sport_club, method: :delete, data: { confirm: 'Are you sure?' } %></td>
	</tr>
	<% end %>
</table>

<br />

