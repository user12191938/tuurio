<h1>Listing sport_doctors</h1>

<table>
  <tr>
    <th>Imagepath</th>
    <th>Name</th>
    <th>Surname</th>
    <th>Description</th>
    <th>Address</th>
    <th>Plz</th>
    <th>City</th>
    <th>Price</th>
    <th>Profession</th>
    <th>Subjectarea</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @sport_doctors.each do |sport_doctor| %>
  <tr>
    <td><%= sport_doctor.imagePath %></td>
    <td><%= sport_doctor.name %></td>
    <td><%= sport_doctor.surname %></td>
    <td><%= sport_doctor.description %></td>
    <td><%= sport_doctor.address %></td>
    <td><%= sport_doctor.plz %></td>
    <td><%= sport_doctor.city %></td>
    <td><%= sport_doctor.price %></td>
    <td><%= sport_doctor.profession %></td>
    <td><%= sport_doctor.subjectArea %></td>
    <td><%= link_to 'Show', sport_doctor %></td>
    <td><%= link_to 'Edit', edit_sport_doctor_path(sport_doctor) %></td>
    <td><%= link_to 'Destroy', sport_doctor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

