<h1>Listing ad_contacts</h1>

<table>
  <tr>
    <th>Adschedule</th>
    <th>Company</th>
    <th>Description</th>
    <th>Name</th>
    <th>Surname</th>
    <th>Position</th>
    <th>Email</th>
    <th>Phone</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @ad_contacts.each do |ad_contact| %>
  <tr>
    <td><%= ad_contact.adSchedule %></td>
    <td><%= ad_contact.company %></td>
    <td><%= ad_contact.description %></td>
    <td><%= ad_contact.name %></td>
    <td><%= ad_contact.surname %></td>
    <td><%= ad_contact.position %></td>
    <td><%= ad_contact.email %></td>
    <td><%= ad_contact.phone %></td>
    <td><%= link_to 'Show', ad_contact %></td>
    <td><%= link_to 'Edit', edit_ad_contact_path(ad_contact) %></td>
    <td><%= link_to 'Destroy', ad_contact, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

