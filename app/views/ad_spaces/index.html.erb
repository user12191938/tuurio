<h1>Listing ad_spaces</h1>

<table>
  <tr>
    <th>Adschedule</th>
    <th>Contact</th>
    <th>Subjectarea</th>
    <th>Price</th>
    <th>Size</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @ad_spaces.each do |ad_space| %>
  <tr>
    <td><%= ad_space.adSchedule %></td>
    <td><%= ad_space.contact %></td>
    <td><%= ad_space.subjectArea %></td>
    <td><%= ad_space.price %></td>
    <td><%= ad_space.size %></td>
    <td><%= link_to 'Show', ad_space %></td>
    <td><%= link_to 'Edit', edit_ad_space_path(ad_space) %></td>
    <td><%= link_to 'Destroy', ad_space, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Ad space', new_ad_space_path %>
